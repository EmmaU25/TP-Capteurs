<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yahlopee.capteurs_tp">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListeActivity" />
        <activity android:name=".accelerometreActivity" />
        <activity android:name=".directionActivity" />
        <activity android:name=".secouerActivity" />
        <activity android:name=".proximiteActivity" />
        <activity android:name=".absenceActivity" />
        <activity android:name=".gpsActivity"></activity>
    </application>

</manifest>